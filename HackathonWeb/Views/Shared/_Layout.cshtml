<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VS_WebApp</title>
    <!-- Basic -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- Mobile Metas -->
    @*<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />*@
    <!-- Site Metas -->
    <meta name="keywords" content="" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <title>VoxScribe</title>

    <!-- bootstrap core css -->
    <link rel="stylesheet" href="css/bootstrap.css">

    <!-- fonts style -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">

    <!--owl slider stylesheet -->
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />

    <!-- font awesome style -->
    <link href="css/font-awesome.min.css" rel="stylesheet" />
    <!-- nice select -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-nice-select/1.1.0/css/nice-select.min.css" integrity="sha256-mLBIhmBvigTFWPSCtvdu6a76T+3Xyt+K571hupeFLg4=" crossorigin="anonymous" />
    <!-- datepicker -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker.css">
    <!-- Custom styles for this template -->
    <link href="css/style.css" rel="stylesheet" />
    <!-- responsive style -->
    <link href="css/responsive.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    
</head>

<body>
    <!-- header section strats -->
    <!-- header section strats -->
    <header class="header_section">
        <div class="header_bottom">
            <div class="container-fluid">
                <nav class="navbar navbar-expand-lg custom_nav-container">
                    <a class="navbar-brand" href="index.html">
                        <span class="visually-hidden">Nav</span>
                        <div class="image-container1">
                        <img src="images/logo.png" style="width:80px;" alt="">
                        </div>
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class=""> </span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                        <div class="d-flex mr-auto flex-column flex-lg-row align-items-center">
                            <ul class="navbar-nav  ">
                                <li class="nav-item active">
                                    <a class="nav-link" href="index.html">Home <span class="sr-only">(current)</span></a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="contact.html">Contact Us</a>
                                </li>
                            </ul>
                        </div>
                        <div class="quote_btn-container">
                            <a href="">
                                <i class="fa fa-user" aria-hidden="true"></i>
                                <span>
                                    Login
                                </span>
                            </a>
                            <a href="">
                                <i class="fa fa-user" aria-hidden="true"></i>
                                <span>
                                    Sign Up
                                </span>
                            </a>
                            <form class="form-inline">
                                <button class="btn  my-2 my-sm-0 nav_search-btn" type="submit" aria-label="Menu" >
                                    <span class="visually-hidden">Submit</span>
                                    <i class="fa fa-search" aria-hidden="true"></i>
                                </button>
                            </form>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </header>
    <!-- end header section -->
    <!-- slider section -->
    <section class="slider_section ">
        <div class="dot_design">
            <img src="images/dots.png" alt="">
        </div>
        <div class="container ">
            <div class="row">
                <div class="col-md-2">
                    </div>
                <div class="col-md-6">
                    <div class="detail-box image-container img">
                        <div class="play_btn">
                            <button id="toggle">
                                <span class="visually-hidden">Button</span>
                                <i class="fa fa-play" aria-hidden="true"></i>
                            </button>
                        </div>
                        <h1>Vox<br>
                            <span>
                                Scribe
                            </span>
                        </h1>
                        <p>
                            <!-- when looking at its layout. The point of using Lorem Ipsum is that it has a more-or-less normal distribution of letters, as opposed to -->
                        </p>
                        <a href="">
                            Contact Us
                        </a>
                    </div>
                </div>
                
            </div>
        </div>
        <div id="customCarousel1" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="container ">
                        <div class="row">
                            <div class="col-md-10">
                                <div class="img-box">
                                    <img src="images/slider-img.webp" class="same-size" alt="">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                <div class="carousel-item">
                    <div class="container ">
                        <div class="row">
                            <div class="col-md-10">
                                <div class="img-box">
                                    <img src="images/slider-img2.jpg" class="same-size" alt="">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="container ">
                        <div class="row">
                            <div class="col-md-10">
                                <div class="img-box">
                                    <img src="images/slider-img3.jpg" class="same-size" alt="">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- end slider section -->
    <!-- book section -->

    <section class="book_section layout_padding">
        <div class="container">
            <div class="justify-content-center">
                <div class="row">
                    <div class="col text-center" style="font-size: 24px;">
                        <code id="result">
                            live transcript is here
                        </code>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="mt-3"></div> <!-- Add a spacer with 'mt-3' class -->
                <!-- Replace 'mt-3' with the desired spacing class (mt for margin-top or mb for margin-bottom) and adjust the number to set the desired space -->
                <div class="col text-center">
                    <!-- Add 'text-center' class here -->
                    <button class="btn btn-primary" type="submit" id="btnCompleteAssessment" >Complete assessment.</button>
                    <div class="mt-3"></div> 
                    <div id="output" class="mt-3"></div>
                    <!-- Add a spacer with 'mt-3' class -->
                    <!-- Replace 'mt-3' with the desired spacing class (mt for margin-top or mb for margin-bottom) and adjust the number to set the desired space -->
                </div>
            </div>
        
            <div class="row">
                <div class="col">
                    <form>
                        <h4>
                            <span>Conduct Assessment</span>
                        </h4>
                        <div class="form-row ">
                            <div class="form-group col-lg-4">
                                <label for="inputAssessmentName">Assessment Name</label>
                                <select name="" class="form-control wide" id="inputAssessmentName">
                                    <option value="Normal distribution ">Assessment 1 </option>
                                    <option value="Normal distribution ">Assessment 2 </option>
                                    <option value="Normal distribution ">Assessment 3 </option>
                                </select>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col">
                                <label for="inputPatientName">Patient's' Name </label>
                                <input type="text" class="form-control" id="inputPatientName" placeholder="">
                            </div>
                            <div class="form-group col">
                                <label for="inputDoctorName">Health Coordinator's Name</label>
                                <input type="text" class="form-control" id="inputDoctorName" placeholder="">
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col">
                                <label for="inputAllergies">Allergies</label>
                                <textarea class="form-control" id="inputAllergies" style="width:100%; height:300px;" placeholder=""></textarea>
                            </div>
                            <div class="form-group col">
                                <label for="inputMedications">Medical Condition</label>
                                <textarea class="form-control" id="inputMedicalCondition" style="width:100%; height:300px;" placeholder=""></textarea>
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="form-group col">
                                <label for="inputVH">Vaccine History</label>
                                <textarea class="form-control" id="inputVH" style="width:100%; height:300px;" placeholder=""></textarea>
                            </div>
                            <div class="form-group col">
                                <label for="inputMH">Medical History</label>
                                <textarea class="form-control" id="inputMH" style="width:100%; height:300px;" placeholder=""></textarea>
                            </div>
                        </div>
                        <div class="form-row ">
                            <label for="inputNote">Summary</label>
                            <textarea class="form-control" id="inputSummary" style="width:100%; height:300px;" placeholder=""></textarea>
                        </div>
                        <div class="btn-box">
                            <button type="submit" class="btn ">Submit Now</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

    <!-- end book section -->
    <!-- about section -->

    <section class="about_section">
        <div class="container  ">
            <div class="row">
                <div class="col-md-6 ">
                </div>
                <div class="col-md-6">
                    <div class="detail-box">
                        <div class="heading_container">
                        </div>
                        <a href="">
                            Read More
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- footer section -->
    <footer class="footer_section">
        <div class="container">
            <p>
                &copy; <span id="displayYear"></span> All Rights Reserved
                @*<a href="https://html.design/">Free Html Templates</a>*@
            </p>
        </div>
    </footer>
    <!-- footer section -->
    <!-- jQery -->
    <script src="js/jquery-3.4.1.min.js"></script>
    <!-- bootstrap js -->
    <script src="js/bootstrap.js"></script>
      <!-- owl slider -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <!-- datepicker -->
    
    <!-- custom js -->
    <script src="js/custom.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
